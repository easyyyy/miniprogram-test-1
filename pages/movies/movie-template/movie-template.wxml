<import src="../stars-template/stars-template.wxml"></import>
<template name="movieTemplate">
    <view class="movie-container" bindtap="clickMovie" data-movie-id="{{movieId}}">
        <image class="movie-image" src="{{coverageUrl}}"></image>
        <text class="movie-title">{{title}}</text>
        <template is="starsTemplate" data="{{stars,average}}"></template>
    </view>
</template>
